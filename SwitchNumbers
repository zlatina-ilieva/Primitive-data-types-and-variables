/*
Declare  two integer variables and assign them with 5 and 10 and after that exchange their values.
 */

using System;

class SwitchInt
{
    static void Main()
    {
        int a = 5, b = 10, tmp;
        tmp = a;
        a = b;
        b = tmp;
        Console.WriteLine("a: " + a);
        Console.WriteLine("b: " + b);
    }
}
